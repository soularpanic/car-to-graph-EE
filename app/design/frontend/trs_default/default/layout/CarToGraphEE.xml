                                </round>
                                <oval>
                                    <title>Oval</title>
                                    <image><url>images/productcategory/shrouds-oval.svg</url></image>
                                    <action>
                                        <minih1>sku:E55R-R, OCULAR-R</minih1>
                                        <minid2so>sku:E55R-R, OCULAR-R</minid2so>
                                        <minid2sx>sku:Apollo2SQ-R, OcularSquare-R</minid2sx>
                                        <fxr30>sku:E55R-R, OCULAR-R</fxr30>
                                        <other>sku:E55R-R, OCULAR-R</other>
